.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page{
  background-color: gray;
  background: ghostwhite;
}

userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #aaa;
}

userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}